    public void problema1() {
        Console.WriteLine("");
        Console.WriteLine("");
        Console.WriteLine("----------------------");
        Console.WriteLine("----- Menu local -----");
        Console.WriteLine("1. Últimos 3 ejercicios de la Tarea 9 ");
        Console.WriteLine("----------------------");
        int varAuxiliarlocal = 0;
        Program problema1 = new Program();
        while (varAuxiliarlocal == 0) {
            Console.WriteLine("");
            Console.WriteLine("");
            Console.WriteLine("--- Menu local ---");
            Console.WriteLine("");
            Console.WriteLine("Opciones ");
            Console.WriteLine("1. Ejercicio 1");
            Console.WriteLine("2. Ejercicio 2");
            Console.WriteLine("3. Ejercicio 3");
            Console.WriteLine("4. Salir ");
            Console.WriteLine("-----------------------");
            Console.WriteLine("Introduce la opcion: ");
            String opcionEntradaLocal;
            opcionEntradaLocal = Console.ReadLine();

            if (opcionEntradaLocal == "1") {
                problema1.problema1_1();
            }
            if (opcionEntradaLocal == "2") {
                problema1.problema1_2();
            }
            if (opcionEntradaLocal == "3") {
                problema1.problema1_3();
            }
            if (opcionEntradaLocal == "4") {
                varAuxiliarlocal = 1;
            }
            if (opcionEntradaLocal != "1" && opcionEntradaLocal != "2" && opcionEntradaLocal != "3" && opcionEntradaLocal != "4") {
                Console.WriteLine("Opcion erronea, introduce una opcion entre 1-4");
            }
        }
    }

    public void problema1_1() {
        Console.WriteLine("");
        Console.WriteLine("----------------------");
        Console.WriteLine("----- Menu local -----");
        Console.WriteLine("1. Últimos 3 ejercicios de la Tarea 9 ");
        Console.WriteLine("Ejercicio 1");
        Console.WriteLine("----------------------");
        //Programa que determine cuál de tres números diferentes es el mayor, y cuál es el menor. (Si los numeros son iguales, enviar mensaje) (Usar IF)
        int num1 = 0;
        int num2 = 0;
        int num3 = 0;
        Console.WriteLine("Se determinara qué número es mayor y cúal menor de 3 valores...");
        Console.WriteLine("Inserte el primer número");
        num1 = int.Parse(Console.ReadLine());
        Console.WriteLine("Inserte el segundo número");
        num2 = int.Parse(Console.ReadLine());
        Console.WriteLine("Inserte el tercer número");
        num3 = int.Parse(Console.ReadLine());
        if (num1 == num2 & num1 == num3 & num2 == num3) {
            Console.WriteLine("Todos los números son iguales, seleccione otros.");
        }
        if (num1 > num2 & num1 > num3) //el primer número es mayor
        {
            if (num1 > num3) //el 2do mayor al 3ro
            {
                Console.WriteLine("El primer número (" + num1 + ") es mayor y el tercer número (" + num3 + ")" + " es menor");
            } else {
                Console.WriteLine("El primer número (" + num1 + ") es mayor y el segundo número (" + num2 + ")" + " es menor");
            }
        }
        if (num2 > num1 & num2 > num3) { //el segundo mayor
            if (num1 > num2) {
                Console.WriteLine("El segundo número (" + num2 + ") es mayor y el tercer número (" + num3 + ")" + " es menor");
            } else {
                Console.WriteLine("El segundo número (" + num2 + ") es mayor y el primer número (" + num1 + ")" + " es menor");
            }
        }
        if (num3 > num1 & num3 > num2) { //El tercero mayor
            if (num1 > num2) {
                Console.WriteLine("El tercer número (" + num3 + ") es mayor y el segundo número (" + num2 + ")" + " es menor");
            } else {
                Console.WriteLine("El tercer número (" + num3 + ") es mayor y el primer número (" + num1 + ")" + " es menor");
            }
        }
		Console.WriteLine("----------------------");
		Console.WriteLine("Digita cualquier tecla y presiona enter");
		String Auxdetente = Console.ReadLine();
		
    }
    public void problema1_2() {
        Console.WriteLine("");
        Console.WriteLine("----------------------");
        Console.WriteLine("----- Menu local -----");
        Console.WriteLine("1. Últimos 3 ejercicios de la Tarea 9 ");
        Console.WriteLine("Ejercicio 2");
        Console.WriteLine("----------------------");
        decimal cal6 = 0;
        decimal cal7 = 0;
        decimal cal8 = 0;
        decimal cal9 = 0;
        decimal cal10 = 0;
        decimal prom1 = 0;
        decimal max = 0;
        decimal min = 0;
        Console.WriteLine("Se determinará el promedio de 5 números");
        Console.WriteLine("Inserte 1er número");
        cal6 = decimal.Parse(Console.ReadLine());
        Console.WriteLine("Inserte 2do número");
        cal7 = decimal.Parse(Console.ReadLine());
        Console.WriteLine("Inserte 3ra número");
        cal8 = decimal.Parse(Console.ReadLine());
        Console.WriteLine("Inserte 4ta número");
        cal9 = decimal.Parse(Console.ReadLine());
        Console.WriteLine("Inserte 5ta número");
        cal10 = decimal.Parse(Console.ReadLine());

        prom1 = (cal6 + cal7 + cal8 + cal9 + cal10) / 5;

        Console.WriteLine("Los números son: " + cal6 + ",  " + cal7 + ",  " + cal8 + ",  " + cal9 + ",  " + cal10 + ", y el promedio es: " + prom1);
        Console.WriteLine("El máximo es " + Math.Max(cal6, cal7));

        if (cal6 > cal7 & cal6 > cal8 & cal6 > cal9 & cal6 > cal10) // el primero es mayor
        {
            if (cal7 > cal8 & cal7 > cal9 & cal7 > cal10) // el segundo mayor 
            {
                if (cal8 > cal9 & cal8 > cal10) //el tercero mayor 
                {
                    if (cal9 > cal10) //el 4to mayor
                    {
                        Console.WriteLine("El 1er número (" + cal6 + ")" + " es mayor y el 5to número (" + cal10 + ")" + " es menor");
                    } else {
                        Console.WriteLine("El 1er número (" + cal6 + ")" + " es mayor y el 4to número (" + cal9 + ")" + " es menor");
                    }
                }
            }
        }
		Console.WriteLine("----------------------");
		Console.WriteLine("Digita cualquier tecla y presiona enter");
		String Auxdetente = Console.ReadLine();
    }
    public void problema1_3() {
        Console.WriteLine("");
        Console.WriteLine("----------------------");
        Console.WriteLine("----- Menu local -----");
        Console.WriteLine("1. Últimos 3 ejercicios de la Tarea 9 ");
        Console.WriteLine("Ejercicio 3");
        Console.WriteLine("----------------------");
        double num10 = 0;
        double num11 = 0;
        double potencia = 0;
        Console.WriteLine("Ejercicio 12: Se elevará un número X a la n potencia");
        Console.WriteLine("Inserte número");
        num10 = double.Parse(Console.ReadLine());
        Console.WriteLine("Inserte número de potencia");
        num11 = double.Parse(Console.ReadLine());
        potencia = Math.Pow(num10, num11);
        //potencia = (num10)^(num11);
        Console.WriteLine("El resultado de elevar (" + num10 + ")" + " a la pontencia (" + num11 + ")" + " es: " + potencia);
		Console.WriteLine("----------------------");
		Console.WriteLine("Digita cualquier tecla y presiona enter");
		String Auxdetente = Console.ReadLine();
    }

    
