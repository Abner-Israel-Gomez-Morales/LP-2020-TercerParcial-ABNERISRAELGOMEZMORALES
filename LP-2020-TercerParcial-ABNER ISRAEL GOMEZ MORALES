        }
    }

    public void problema3_1() {
        Console.WriteLine("");
        Console.WriteLine("----------------------");
        Console.WriteLine("----- Menu local -----");
        Console.WriteLine("3. Primero 5 Ejercicios de la Tarea 11 ");
        Console.WriteLine("1. Ejercicio 1");
        Console.WriteLine("----------------------");
        //1.- Muestre en pantalla los números del 1 al 100 en orden ascendente

        Console.WriteLine("Problema 1");

        int x;
        for (x = 1; x <= 100; x++) {
            Console.WriteLine("Repeticion # " + x);
        }
		Console.WriteLine("----------------------");
		Console.WriteLine("Digita cualquier tecla y presiona enter");
		String Auxdetente = Console.ReadLine();
    }
    public void problema3_2() {
        Console.WriteLine("");
        Console.WriteLine("----------------------");
        Console.WriteLine("----- Menu local -----");
        Console.WriteLine("3. Primero 5 Ejercicios de la Tarea 11 ");
        Console.WriteLine("2. Ejercicio 2");
        Console.WriteLine("----------------------");
        //2.- Muestre los números del 100 al 0 en orden descendente 

        Console.WriteLine("Problema 2");

        int a;
        for (a = 100; a >= 0; a--) {
            Console.WriteLine("Repeticion # " + a);
        }
		Console.WriteLine("----------------------");
		Console.WriteLine("Digita cualquier tecla y presiona enter");
		String Auxdetente = Console.ReadLine();
    }
    public void problema3_3() {
        Console.WriteLine("");
        Console.WriteLine("----------------------");
        Console.WriteLine("----- Menu local -----");
        Console.WriteLine("3. Primero 5 Ejercicios de la Tarea 11 ");
        Console.WriteLine("3. Ejercicio 3");
        Console.WriteLine("----------------------");
        //3.- Imprimir la suma de los primero 100 numeros 
        Console.WriteLine("...");
        Console.WriteLine("=== EJERCICIO 3 ====");

        int y;
        int sum = 0;
        for (y = 1; y <= 100; y++) {
            sum = sum + y;
            Console.WriteLine("Suma de : " + (y - 1) + " mas " + (sum - (y - 1)) + " es igual a " + sum);
        }
		Console.WriteLine("----------------------");
		Console.WriteLine("Digita cualquier tecla y presiona enter");
		String Auxdetente = Console.ReadLine();
    }

    public void problema3_4() {
        Console.WriteLine("");
        Console.WriteLine("----------------------");
        Console.WriteLine("----- Menu local -----");
        Console.WriteLine("3. Primero 5 Ejercicios de la Tarea 11 ");
        Console.WriteLine("4. Ejercicio 4");
        Console.WriteLine("----------------------");
        //4.- Introducir tantas frases como el usuario quiera y contar cuantas fueron.
        Console.WriteLine("..");
        Console.WriteLine("Problema 4");
        Console.WriteLine("..");

        string frase;
        int z1 = 0;
        Console.WriteLine("Ingresa todas las frases que quieras");
        Console.WriteLine("Cuando quieras terminar solo debes escribr la palabra <fin>");
        for (frase = ""; frase != "fin"; frase = Console.ReadLine()) {
            z1 = z1 + 1;
        }
        Console.WriteLine("El numero de frases ingresadas es de: " + (z1 - 1));
		Console.WriteLine("----------------------");
		Console.WriteLine("Digita cualquier tecla y presiona enter");
		String Auxdetente = Console.ReadLine();
    }
    public void problema3_5() {
        Console.WriteLine("");
        Console.WriteLine("----------------------");
        Console.WriteLine("----- Menu local -----");
        Console.WriteLine("3. Primero 5 Ejercicios de la Tarea 11 ");
        Console.WriteLine("5. Ejercicio 5");
        Console.WriteLine("----------------------");
        int a, b, e = 0, a1, b1;
        int c = 1;
        int d = 1, q1 = 0;
        int suma = 0, sumapar = 0;
        int par = 0;
        int sumaimpar = 0;
        Console.WriteLine("Ingrese dos números: ");
        a = Int32.Parse(Console.ReadLine());
        b = Int32.Parse(Console.ReadLine());

        for (c = 1;
            (a >= 0 && b >= 0 && c <= 1); c++) {
            for (d = 1;
                (a < b && d <= 1); d++) {
                a = a + 1;
                b = b - 1;
                a1 = a;
                b1 = b;
                for (a = a1;
                    (a <= b); a++) {
                    Console.WriteLine("#" + a);
                    for (par = 0;
                        (a % 2 == 0 && par < 1); par++) {
                        sumapar = sumapar + 1;
                    }
                    par = 0;
                    for (par = 0;
                        (a % 2 == 1 && par < 1); par++) {
                        sumaimpar = sumaimpar + 1;
                        e = e + a;
                    }
                    par = 0;
                    suma = suma + 1;

                }
                Console.WriteLine("Entre ambos numeros ingresados hay un total de " + (suma) + " números");
                Console.WriteLine("Entre ambos numeros ingresados hay un total de " + (sumapar) + " números pares");
                Console.WriteLine("Entre ambos numeros ingresados hay un total de " + (sumaimpar) + " números impares");
                Console.WriteLine("La suma de los números impares es de: " + e);
                q1 = d;
            }
            for (d = q1;
                (a > b && d < 1); d++) {
                a = a - 1;
                b = b + 1;
                a1 = a;
                b1 = b;
                for (b = b1;
                    (b <= a); b++) {
                    for (par = 0;
                        (b % 2 == 0 && par < 1); par++) {
                        sumapar = sumapar + 1;
                    }
                    par = 0;
                    for (par = 0;
                        (b % 2 == 1 && par < 1); par++) {
                        sumaimpar = sumaimpar + 1;
                        e = e + b;
                    }
                    par = 0;
                    Console.WriteLine("#" + b);
                    suma = suma + 1;
                }
                Console.WriteLine("Entre ambos numeros ingresados hay un total de " + (suma) + " números");
                Console.WriteLine("Entre ambos numeros ingresados hay un total de " + (sumapar) + " números pares");
                Console.WriteLine("Entre ambos numeros ingresados hay un total de " + (sumaimpar) + " números impares");
                Console.WriteLine("La suma de los números impares es de: " + e);
            }
			Console.WriteLine("----------------------");
		Console.WriteLine("Digita cualquier tecla y presiona enter");
		String Auxdetente = Console.ReadLine();
        }
        for (c = 1;
            ((a <= 0 || b <= 0) && c <= 1); c++) {
            Console.WriteLine("El o los numeros ingresados son negativos");
        }
        for (e = 0;
            (a == b && e < 1); e++) {
            Console.WriteLine("Los números ingresados son iguales");
        }
    }


