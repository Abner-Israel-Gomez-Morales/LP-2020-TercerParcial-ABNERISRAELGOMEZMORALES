    //////////////////////////////////////////////////////////////////////////////
    public void problema4() {
        Console.WriteLine("");
        Console.WriteLine("");
        Console.WriteLine("----------------------");
        Console.WriteLine("----- Menu local -----");
        Console.WriteLine("4. Últimos 5 ejercicios de la Tarea 12 ");
        Console.WriteLine("----------------------");
        int varAuxiliarlocal = 0;
        Program problema4 = new Program();
        while (varAuxiliarlocal == 0) {
            Console.WriteLine("");
            Console.WriteLine("");
            Console.WriteLine("--- Menu local ---");
            Console.WriteLine("");
            Console.WriteLine("Opciones ");
            Console.WriteLine("1. Ejercicio 1");
            Console.WriteLine("2. Ejercicio 2");
            Console.WriteLine("3. Ejercicio 3");
            Console.WriteLine("4. Ejercicio 4");
            Console.WriteLine("5. Ejercicio 5");
            Console.WriteLine("6. Salir ");
            Console.WriteLine("-----------------------");
            Console.WriteLine("Introduce la opcion: ");
            String opcionEntradaLocal;
            opcionEntradaLocal = Console.ReadLine();

            if (opcionEntradaLocal == "1") {
                problema4.problema4_1();
            }
            if (opcionEntradaLocal == "2") {
                problema4.problema4_2();
            }
            if (opcionEntradaLocal == "3") {
                problema4.problema4_3();
            }
            if (opcionEntradaLocal == "4") {
                problema4.problema4_4();
            }
            if (opcionEntradaLocal == "5") {
                problema4.problema4_5();
            }
            if (opcionEntradaLocal == "6") {
                varAuxiliarlocal = 1;
            }
            if (opcionEntradaLocal != "1" && opcionEntradaLocal != "2" && opcionEntradaLocal != "3" && opcionEntradaLocal != "4" && opcionEntradaLocal != "5" && opcionEntradaLocal != "6") {
                Console.WriteLine("Opcion erronea, introduce una opcion entre 1-6");
            }
        }
    }

    public void problema4_1() {
        Console.WriteLine("");
        Console.WriteLine("----------------------");
        Console.WriteLine("----- Menu local -----");
        Console.WriteLine("4. Últimos 5 ejercicios de la Tarea 12 ");
        Console.WriteLine("1. Ejercicio 1");
        Console.WriteLine("----------------------");
        //8. Algoritmo que muestre una lista de todos los números pares que hay entre dos números cualquiera y que nos
        //diga cuantos son. (Ejemplo: Se leen los números 8 y 17, el resultado del algoritmo debe ser 10,12,14,16, Entre
        //el 8 y el 17 hay 5 números pares)
        Console.WriteLine("====================");
        Console.WriteLine("=== EJERCICIO 8 ====");
        Console.WriteLine("====================");
        int g1, g2, g3 = 0, g4 = 0, g5 = 0;
        int g6, g7;
        Console.WriteLine("Ingrese dos números: ");
        g1 = Int32.Parse(Console.ReadLine());
        g2 = Int32.Parse(Console.ReadLine());
        g6 = g1;
        g7 = g2;
        while (g1 < g2 && g3 < 1) {
            g1 = g1 + 1;
            g2 = g2 - 1;
            while (g1 <= g2) {
                while (g1 % 2 == 0 && g4 < 1) {
                    Console.WriteLine("#: " + g1);
                    g4++;
                    g5 = g5 + 1;
                }
                g4 = 0;
                g1++;
            }
            g3++;
            Console.WriteLine("Entre el " + g6 + " y el " + g7 + " hay " + g5 + " números pares.");
        }
        while (g2 < g1 && g3 < 1) {
            g1 = g1 - 1;
            g2 = g2 + 1;
            while (g2 <= g1) {
                while (g2 % 2 == 0 && g4 < 1) {
                    Console.WriteLine("#: " + g2);
                    g4++;
                    g5 = g5 + 1;
                }
                g4 = 0;
                g2++;
            }
            g3++;
            Console.WriteLine("Entre el " + g7 + " y el " + g6 + " hay " + g5 + " números pares.");
        }
        while (g1 == g2 && g3 < 1) {
            Console.WriteLine("Los números ingresados son iguales");
            g3++;
        }
		Console.WriteLine("----------------------");
		Console.WriteLine("Digita cualquier tecla y presiona enter");
		String Auxdetente = Console.ReadLine();
    }
    public void problema4_2() {
        Console.WriteLine("");
        Console.WriteLine("----------------------");
        Console.WriteLine("----- Menu local -----");
        Console.WriteLine("4. Últimos 5 ejercicios de la Tarea 12 ");
        Console.WriteLine("2. Ejercicio 2");
        Console.WriteLine("----------------------");
        //9. Algoritmo que muestre una lista de todos los números impares que hay entre dos números cualquiera. (Ejemplo:
        //Se leen los números 8 y 17, el resultado del algoritmo debe ser 9,11,13,15, Entre el 8 y el 17 hay 4 números
        //impares)
        Console.WriteLine("--");
        Console.WriteLine("-- Problema 9--");
        Console.WriteLine("--");
		
        int h1, h2, h3 = 0, h4 = 0, h5 = 0;
        int h6, h7;
        Console.WriteLine("Ingrese dos números: ");
        h1 = Int32.Parse(Console.ReadLine());
        h2 = Int32.Parse(Console.ReadLine());
        h6 = h1;
        h7 = h2;
        while (h1 < h2 && h3 < 1) {
            h1 = h1 + 1;
            h2 = h2 - 1;
            while (h1 <= h2) {
                while (h1 % 2 == 1 && h4 < 1) {
                    Console.WriteLine("#: " + h1);
                    h4++;
                    h5 = h5 + 1;
                }
                h4 = 0;
                h1++;
            }
            h3++;
            Console.WriteLine("Entre el " + h6 + " y el " + h7 + " hay " + h5 + " números impares.");
        }
        while (h2 < h1 && h3 < 1) {
            h1 = h1 - 1;
            h2 = h2 + 1;
            while (h2 <= h1) {
                while (h2 % 2 == 1 && h4 < 1) {
                    Console.WriteLine("#: " + h2);
                    h4++;
                    h5 = h5 + 1;
                }
                h4 = 0;
                h2++;
            }
            h3++;
            Console.WriteLine("Entre el " + h7 + " y el " + h6 + " hay " + h5 + " números impares.");
        }
        while (h1 == h2 && h3 < 1) {
            Console.WriteLine("Los números ingresados son iguales");
            h3++;
        }
		Console.WriteLine("----------------------");
		Console.WriteLine("Digita cualquier tecla y presiona enter");
		String Auxdetente = Console.ReadLine();
    }
    public void problema4_3() {
        Console.WriteLine("");
        Console.WriteLine("----------------------");
        Console.WriteLine("----- Menu local -----");
        Console.WriteLine("4. Últimos 5 ejercicios de la Tarea 12 ");
        Console.WriteLine("3. Ejercicio 3");
        Console.WriteLine("----------------------");
        //10. Algoritmo para leer 5 números, obtener su promedio, el mayor y el menor de esos números.
        
		Console.WriteLine("----------------------");
        Console.WriteLine("--   Ejercicio 10   --");
        Console.WriteLine("----------------------");
        int c1, c2, c3, c4, c5, promedio;
		int y1 = 0, y2 = 0;
		Console.WriteLine("Ingrese cinco números: ");
		c1 = Int32.Parse(Console.ReadLine());
		c2 = Int32.Parse(Console.ReadLine());
		c3 = Int32.Parse(Console.ReadLine());
		c4 = Int32.Parse(Console.ReadLine());
		c5 = Int32.Parse(Console.ReadLine());
		promedio = (c1 + c2 + c3 + c4 + c5 )/5;
		Console.WriteLine("El promedio de los 5 números ingresados es de:" + promedio);

		while(c1==c2 && c1==c3 && c1==c4 && c1==c5 && y1<1 && y2<1)
		{
			Console.WriteLine("Todos los números ingresados son iguales");
			y1 ++ ;
			y2 ++ ;
		}
		while((c1>c2 || c1>=c2) && (c1>c3 || c1>=c3) && (c1>c4 || c1>=c4) && (c1>c5 || c1>=c5) && (y1<1))
		{
			Console.WriteLine("El número mas alto es el: "+ c1+" que fue el primer numero en ser ingresado");
			y1 ++ ;
		}
		while((c2>c1 || c2>=c1) && (c2>c3 || c2>=c3) && (c2>c4 || c2>=c4) && (c2>c5 || c2>=c5) && (y1<1))
		{
			Console.WriteLine("El número mas alto es el: "+ c2+" que fue el segundo numero en ser ingresado");
			y1 ++ ;
		}
		while((c3>c2 || c3>=c2) && (c3>c1 || c3>=c1) && (c3>c4 || c3>=c4) && (c3>c5 || c3>=c5) && (y1<1))
		{
			Console.WriteLine("El número mas alto es el: "+ c3+" que fue el tercer numero en ser ingresado");
			y1 ++ ;
		}
		while((c4>c2 || c4>=c2) && (c4>c3 || c4>=c3) && (c4>c1 || c4>=c1) && (c4>c5 || c4>=c5) && (y1<1))
		{
			Console.WriteLine("El número mas alto es el: "+ c4+" que fue el cuarto numero en ser ingresado");
			y1 ++ ;
		}
		while((c5>c2 || c5>=c2) && (c5>c3 || c5>=c3) && (c5>c4 || c5>=c4) && (c5>c1 || c5>=c1) && (y1<1))
		{
			Console.WriteLine("El número mas alto es el: "+ c5+" que fue el quinto numero en ser ingresado");
			y1 ++ ;
		}
		while((c1<c2 || c1<=c2) && (c1<c3 || c1<=c3) && (c1<c4 || c1<=c4) && (c1<c5 || c1<=c5) && (y2<1))
		{
			Console.WriteLine("El número mas pequeño es el: "+ c1+" que fue el primer numero en ser ingresado");
			y2 ++ ;
		}
		while((c2<c1 || c2<=c1) && (c2<c3 || c2<=c3) && (c2<c4 || c2<=c4) && (c2<c5 || c2<=c5) && (y2<1))
		{
			Console.WriteLine("El número mas pequeño es el: "+ c2+" que fue el segundo numero en ser ingresado");
			y2 ++ ;
		}
		while((c3<c2 || c3<=c2) && (c3<c1 || c3<=c1) && (c3<c4 || c3<=c4) && (c3<c5 || c3<=c5) && (y2<1))
		{
			Console.WriteLine("El número mas pequeño es el: "+ c3+" que fue el tercer numero en ser ingresado");
			y2 ++ ;
		}
		while((c4<c2 || c4<=c2) && (c4<c3 || c4<=c3) && (c4<c1 || c4<=c1) && (c4<c5 || c4<=c5) && (y2<1))
		{
			Console.WriteLine("El número mas pequeño es el: "+ c4+" que fue el cuarto numero en ser ingresado");
			y2 ++ ;
		}
		while((c5<c2 || c5<=c2) && (c5<c3 || c5<=c3) && (c5<c4 || c5<=c4) && (c5<c1 || c5<=c1) && (y2<1))
		{
			Console.WriteLine("El número mas pequeño es el: "+ c5+" que fue el quinto numero en ser ingresado");
			y2 ++ ;
        }
		Console.WriteLine("----------------------");
		Console.WriteLine("Digita cualquier tecla y presiona enter");
		String Auxdetente = Console.ReadLine();
    }

    public void problema4_4() {
        Console.WriteLine("");
        Console.WriteLine("----------------------");
        Console.WriteLine("----- Menu local -----");
        Console.WriteLine("4. Últimos 5 ejercicios de la Tarea 12 ");
        Console.WriteLine("4. Ejercicio 4");
        Console.WriteLine("----------------------");
        //11. Algoritmo “Criba de Eratóstenes”, para saber los números primos que hay entre dos números. 
		// (Consulte anexos en la Tarea 5)
        Console.WriteLine("----------------------");
        Console.WriteLine("---- Ejercicio 11 ---- ");
        Console.WriteLine("----------------------");
        int divisible = 0;
        int inicio;
        int p1 = 0, p2 = 0, p3 = 0;
        Console.WriteLine("Ingrese un número: ");
        inicio = Int32.Parse(Console.ReadLine());
        Console.WriteLine("Ingresa hasta que número quieras que imprima los números primos");
        int numero_final = int.Parse(Console.ReadLine());
        while (inicio == numero_final && p2 < 1) {
            Console.WriteLine("Los numeros ingresados son iguales");
            p2++;
        }
        inicio = inicio + 1;
        numero_final = numero_final - 1;
        while (inicio < numero_final) {
            int i = 1;
            while (i <= inicio) {
                while (inicio % i == 0 && p1 < 1) {
                    divisible++;
                    p1++;
                }
                p1 = 0;
                while (divisible > 2) {
                    break;
                }
                p2 = 0;
                i++;
            }
            while (divisible == 2 && p3 < 1) {
                Console.WriteLine("#" + inicio + " es primo");
                p3++;
            }
            p3 = 0;
            divisible = 0;
            inicio++;
        }
        inicio = inicio - 1;
        numero_final = numero_final + 1;
        while (numero_final < inicio) {
            int i = 1;
            while (i <= numero_final) {
                while (numero_final % i == 0 && p1 < 1) {
                    divisible++;
                    p1++;
                }
                p1 = 0;
                while (divisible > 2) {
                    break;
                }
                p2 = 0;
                i++;
            }
            while (divisible == 2 && p3 < 1) {
                Console.WriteLine("#" + numero_final + " es primo");
                p3++;
            }
            p3 = 0;
            divisible = 0;
            numero_final++;
        }
		 Console.WriteLine("----------------------");
		Console.WriteLine("Digita cualquier tecla y presiona enter");
		String Auxdetente = Console.ReadLine();
    }
    public void problema4_5() {
        Console.WriteLine("");
        Console.WriteLine("----------------------");
        Console.WriteLine("----- Menu local -----");
        Console.WriteLine("4. Últimos 5 ejercicios de la Tarea 12 ");
        Console.WriteLine("5. Ejercicio 5");
        Console.WriteLine("----------------------");
        //12.-Programa que resuelva el problema de la pelea de Pokemon. (Consultar Tareas 3 y 4)
        
        Console.WriteLine("--- Ejercicio 12 ---");
        Console.WriteLine("---");
        Console.WriteLine("Bienvenido a una pelea mas de pokemon");
        Console.WriteLine("---");
        Console.WriteLine("En esta ocacion se enfrentara:");
        Console.WriteLine("PIKACHU vs JIGGLYPUFF");
        Console.WriteLine("---");
        Console.WriteLine("  Informacion  ");
        Console.WriteLine("---");
        Console.WriteLine("Cuando ataca PIKACHU le quita 55% de vida a su oponente");
        Console.WriteLine("Cuando ataca JIGGLYPUFF le quita 45% de vida a su oponente");
        Console.WriteLine("Ambos poquemones inician con un 100% de vida al inicio del primer juego");
        Console.WriteLine("---");

        int z1 = 100, x1 = 100, turno;
        int t1 = 0, t2 = 0;
        Console.WriteLine("iniciara el juego atacando, elige el turno");
        Console.WriteLine("Turno de PIKACHU = 1");
        Console.WriteLine("Turno de JIGGLYPUFF = 2");
        Console.WriteLine("Ingrese el turno (1 o 2)");
        turno = Int32.Parse(Console.ReadLine());

        while ((turno > 0 && turno < 3) && t1 < 3) {

            while (turno == 1 && z1 > 0 && x1 > 0) {
                Console.WriteLine("                                                   ");
                Console.WriteLine(" - JUGANDO -- ");
                Console.WriteLine("                                                   ");
                Console.WriteLine("--TURNO 1--");
                Console.WriteLine("PIKACHU ataca, quitandole 55% de vida a JIGGLYPUFF.");
                x1 = x1 - 55;
                turno++;
                Console.WriteLine("La vida de PIKACHU es de: " + z1 + " %");
                Console.WriteLine("La vida de JIGGLYPUFF es de: " + x1 + " %");
            }
            while (turno == 2 && z1 > 0 && x1 > 0) {
                Console.WriteLine("                                                   ");
                Console.WriteLine("--Jugando--");
                Console.WriteLine("                                                   ");
                Console.WriteLine("--TURNO 2--");
                Console.WriteLine("JIGGLYPUFF ataca, quitandole 45% de vida a PIKACHU.");
                z1 = z1 - 45;
                turno++;
                Console.WriteLine("La vida de PIKACHU es de: " + z1 + " %");
                Console.WriteLine("La vida de JIGGLYPUFF es de: " + x1 + " %");
            }
            turno = 1;
            while (z1 < 0 && t2 < 1) {
                Console.WriteLine("---");
                Console.WriteLine("Lo sentimos el pokemon PIKACHU a muerto, por lo tanto JIGGLYPUFF a ganado la partida");
                Console.WriteLine("La vida del ganador es de: " + x1 + " %");
                t2++;
            }
                while (x1 < 0 && t2 < 1) {
                Console.WriteLine("---");
                Console.WriteLine("Lo sentimos el pokemon JIGGLYPUFF a muerto, por lo tanto PIKACHU a ganado la partida");
                Console.WriteLine("La vida del ganador es de: " + z1 + " %");
                t2++;
            }
            t1++;
        }
            while ((turno < 0 || turno > 2) && t1 < 1) {
            t1++;
            Console.WriteLine("Ingresaste un turno no valido");
            Console.WriteLine("Nada mas tenias que ingresar 1 o 2");
        }
		Console.WriteLine("----------------------");
		Console.WriteLine("Digita cualquier tecla y presiona enter");
		String Auxdetente = Console.ReadLine();
    }


    public void problema5() {
