using System;

public class Program {
    public static void Main() {
        int varAuxiliarGlobal = 0;
        Program problema = new Program();

        while (varAuxiliarGlobal == 0) {
            Console.WriteLine("--- Menu Principal ---");
            Console.WriteLine("");
            Console.WriteLine("Opciones ");
            Console.WriteLine("1. Últimos 3 ejercicios de la Tarea 9 ");
            Console.WriteLine("2. Ejercicios 2, 3 y 4 de la Tarea 10 ");
            Console.WriteLine("3. Primero 5 Ejercicios de la Tarea 11 ");
            Console.WriteLine("4. Últimos 5 ejercicios de la Tarea 12 ");
            Console.WriteLine("5. Los ejercicios de la Tarea 13 ");
            Console.WriteLine("6. Los ejercicios de la Tarea 14 ");
            Console.WriteLine("7. Salir ");
            Console.WriteLine("-----------------------");
            Console.WriteLine("Introduce la opcion: ");
            int opcionEntrada;
            opcionEntrada = int.Parse(Console.ReadLine());

            switch (opcionEntrada) {
                case 1:
                    problema.problema1();
                    break;
                case 2:
                    problema.problema2();
                    break;
                case 3:
                    problema.problema3();
                    break;
                case 4:
                    problema.problema4();
                    break;
                case 5:
                    problema.problema5();
                    break;
                case 6:
                    problema.problema6();
                    break;
                case 7:
                    varAuxiliarGlobal = 1;
                    Console.WriteLine("");
                    Console.WriteLine("");
                    Console.WriteLine("* Abner Israel Gomez Morales");
                    Console.WriteLine("");
                    break;
                default:
                    Console.WriteLine("Opcion erronea, introduce una opcion entre 1-7");
                    break;
            }
            Console.WriteLine("");
            Console.WriteLine("");
        }
    }

    //////////////////////////////////////////////////////////////////////////////
